<body class="nav-fixed">
  <div id="layoutSidenav">
    
    <div id="layoutSidenav_content">
    <main>
      <header class="page-header page-header-dark bg-gradient-primary-to-secondary pb-10">
        <div class="container-xl px-4">
          <div class="page-header-content pt-4">
            <div class="row align-items-center justify-content-between">
              <div class="col-auto mt-4">
                <h1 class="page-header-title">
                  <div class="page-header-icon"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-filter"><polygon points="22 3 2 3 10 12.46 10 19 14 21 14 12.46 22 3"></polygon></svg></div>
                  Grande Tesouraria
                </h1>
                <div class="page-header-subtitle">Demostrativos mensais do financeiro da Grande Loja Maçônica do Piauí</div>
              </div>
            </div>
          </div>
        </div>
      </header>
      <div class="container-xl px-4 mt-n10">
                        <!-- Wizard card example with navigation-->
                        <div class="card">
                            <div class="card-header border-bottom">
                                <!-- Wizard navigation-->
                                
                            <div class="card-body">
                                <div class="tab-content" id="cardTabContent">
                                    <!-- Wizard tab pane item 1-->
                                    <div class="tab-pane py-5 py-xl-10 fade show active" id="wizard1" role="tabpanel" aria-labelledby="wizard1-tab">
                                        <div class="row justify-content-center">
                                            <div class="col-xxl-10 col-xl-10">
                                            
                                                <h2 class="text-primary">Painel de Cadastro</h2>
                                                <h5 class="card-title mb-4">Informe os dados do Balencetes</h5> 
                                                <form>
                                                <div class="card-body">
                                <div class="dataTable-wrapper dataTable-loading no-footer sortable searchable fixed-columns">
                                    <div class="dataTable-top">
                                        
           
                                        <table id="datatablesSimple" class="dataTable-table table-bordered border-primary">
                                        <%= link_to new_backoffice_balancesheet_path, class:"btn btn-success btn-circle btn-sm", style:"margin-top: -5px" do %>
                                        <i class="fas fa-plus"></i>
                                        <% end %>

                                    <thead class="table-primary" >
                                        <tr>
                                        <th data-sortable="" style="width: 15.4439%;"><a href="#" class="dataTable-sorter">ID</a></th>
                                        <th data-sortable="" style="width: 15.4439%;"><a href="#" class="dataTable-sorter">Número</a></th>
                                        <th data-sortable="" style="width: 23.1658%;"><a href="#" class="dataTable-sorter">Data</a></th>
                                        <th data-sortable="" style="width: 12.7709%;"><a href="#" class="dataTable-sorter">Resumo</a></th>
                                        <th data-sortable="" style="width: 7.49919%;"><a href="#" class="dataTable-sorter">Arquivos</a></th>
                                        <th data-sortable="" style="width: 11.8799%;"><a href="#" class="dataTable-sorter">Ação</a></th>
                                        </tr>
                                    </thead>
                                    <% @balancesheets.each do |balancesheet| %>
                                    <tbody>
                                        <tr>
                                            <td><%= balancesheet.id %></td>
                                            <td><%= balancesheet.number %></td>
                                            <td><%= balancesheet.date %></td>
                                            <td><%= balancesheet.resume %></td>
                                            <td><%= link_to("Balancete", balancesheet.balancheetpdf_url, target: '_blank' ) %></td>
                                            <td><button class="btn btn-datatable btn-icon btn-transparent-dark me-2"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-more-vertical">
                                                    <circle cx="12" cy="12" r="1"></circle></svg>
                                                    </button>
                                                    <button class="btn btn-datatable btn-icon btn-transparent-dark"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-trash-2">
                                                    <polyline points="3 6 5 6 21 6"></polyline>
                                                    <path d="M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2"></path>
                                                    <line x1="10" y1="11" x2="10" y2="17"></line>
                                                    <line x1="14" y1="11" x2="14" y2="17"></line>
                                                    </svg>
                                                    </button></td>
                                        </tr>
                                        <% end %>
                                    </tbody>
                                </table>
                                <%= paginate @balancesheets %>
                            </div
                        </div>
                        </div>
                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
    </main>
    </div>
  </div>
</body>
<div class="container-fluid">

  <h1 class="h3 mb-2 text-gray-800">Balancete</h1>
    <p class="mb-4">Balancete da Grande Loja Maçônica do Piauí.</p>
    <% if notice %>
    <div class="alert alert-success alert-dismissable">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
      <%= notice %>
    </div>
    <% end %>
      <div class="card shadow mb-4">
        <div class="card-header py-3">
          <h6 class="m-0 font-weight-bold text-primary">Balancete
          <%= link_to new_backoffice_balancesheet_path, class:"btn btn-success btn-circle btn-sm", style:"margin-top: -5px" do %>
            <i class="fas fa-plus"></i>
            <% end %>
          </h6>
          </div>
          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                  <tr>
                  <th>#</th>
                    <th>Número</th>
                    <th>Data</th>
                    <th>Resumo</th>
                    <th>Arquivos</th>
                    <th>Ação</th>
                  </tr>
                </thead>
                <tfoot>
                  <tr>
                    <th>#</th>
                    <th>Número</th>
                    <th>Data</th>
                    <th>Resumo</th>
                    <th>Arquivos</th>
                    <th>Ação</th>

                  </tr>
                </tfoot>
                  <% @balancesheets.each do |balancesheet| %>
                <tbody>
                  <tr>
                    <td><%= balancesheet.id %></td>
                    <td><%= balancesheet.number %></td>
                    <td><%= balancesheet.date %></td>
                    <td><%= balancesheet.resume %></td>
                    <td><%= link_to("Balancete", balancesheet.balancheetpdf_url, target: '_blank' ) %></td>
                    <td>
                  <%= link_to edit_backoffice_balancesheet_path(balancesheet), class: 'btn btn-primary btn-circle btn-sm' do %>
                    <i class="fas fa-edit"></i>
                  <% end %>
                  <a href="#" class="btn btn-danger btn-circle btn-sm">
                    <i class="fas fa-trash"></i>
                  </a>
                  </td>
                  </tr>
                  <% end %>
                  </tbody>
                </table>
        <%= paginate @balancesheets %>
              </div>
            </div>
          </div>
        </div>
